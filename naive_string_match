def naive_string_match(text, pattern):
    n = len(text)
    m = len(pattern)
    positions = []

   
    for i in range(n - m + 1):
        match = True
        for j in range(m):
            if text[i + j] != pattern[j]:
                match = False
                break
        if match:
            positions.append(i) 

    return positions



text = "Hamalsaran"
pattern = "sar"

result = naive_string_match(text, pattern)
print("Պատկերն (pattern) գտնվեց դիրքերում:", result)
